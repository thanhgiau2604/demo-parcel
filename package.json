{
  "name": "parcel-demo",
  "version": "1.0.0",
  "license": "MIT",
  "source": "src/pages/index.html",
  "parcel-namer-rewrite": {
    "hashing": "never",
    "disable": false,
    "rules": {
      "src/pages/index.html": "index.html"
    }
  },
  "scripts": {
    "compile": "parcel build \"src/pages/**/*.html\"",
    "dev": "parcel \"src/pages/**/*.html\" --no-cache"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.8.0",
    "parcel": "^2.8.0",
    "parcel-namer-rewrite": "^2.0.0-rc.3",
    "parcel-namer-without-hash": "^0.0.1",
    "posthtml-include": "^1.7.4",
    "prettier": "^2.7.1"
  }
}
